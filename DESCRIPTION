Package: autocogs
Title: Automatic Cognostic Summaries
Version: 0.1.4
Authors@R: c(
    person("Barret", "Schloerke", , "schloerke@gmail.com", role = "aut",
           comment = c(ORCID = "0000-0001-9986-114X")),
